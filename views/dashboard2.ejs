                <table class="table align-items-center table-flush">
                  <thead class="thead-light">
                    <tr>
                      <th scope="col">Title</th>
                      <th scope="col">Date</th>
                      <th scope="col">Time</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if(monthlyMeeting.length > 0){ %> <%
                    monthlyMeeting.forEach(meeting => { %>
                    <tr class="">
                      <td><%= meeting.title %></td>
                      <td><%= meeting.startAt.toUTCString() %></td>
                      <td><%= meeting.time %></td>
                    </tr>
                    <% }) %> <% } else { %>
                    <td>No scheduled meetings this month.</td>
                    <% } %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
        <!-- Footer -->
        <%- include('./partials/dashFooter.ejs') %>
      </div>
    </div>
